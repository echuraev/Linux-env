! Color scheme

! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white

URxvt*color0:    #000000
URxvt*color1:    #C00000
URxvt*color2:    #00C000
URxvt*color3:    #C0C000
URxvt*color4:    #0C00C0
URxvt*color5:    #AA00AA
URxvt*color6:    #00AAAA
URxvt*color7:    #FFFFFF

! Light
! color8     darkgray
! color9     red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    lightgray

URxvt*color8:     #212121
URxvt*color9:     #FF8080
URxvt*color10:    #80FF80
URxvt*color11:    #FFFF88
URxvt*color12:    #99CCFF
URxvt*color13:    #B14CC2
URxvt*color14:    #9BFFFF
URxvt*color15:    #D0D0D0

! Background color
URxvt*background: #212121
! Foreground color
URxvt*foreground: #C0C0C0
! Highlight underlined text (links)
URxvt.underlineColor: blue

! Cursor color
URxvt.cursorColor: #C0C0C0
! Cursor blinking
URxvt.cursorBlink: true

! Set fount
URxvt.font: -schumacher-*-medium-*-*-*-14-*-*-*-*-*-*-*
! URxvt.font: xft:Droid Sans Mono:pixelsize=11:antialias=true
! URxvt.boldFont: xft:Droid Sans Mono:bold:pixelsize=11:antialias=true

! Enable scrollbar
URxvt*scrollBar: true
URxvt*scrollBar_right: true
URxvt.scrollstyle: plain
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

! Load plugins
URxvt.perl-lib:         $HOME/.config/urxvt/perl/
URxvt.perl-ext-common:  default,matcher,url-select,tabbed

! Plugins
! Links are clickable on left mouse button
URxvt.matcher.button: 1

! Transparent
URxvt.transparent: true
! Decreasing (-100 .. -1) or increasing (0 .. 100) highlight of transparent background
URxvt.shading: 20

! Window initial size
URxvt.geometry: 80x25
! Terminal history size
URxvt*saveLines: 7000
